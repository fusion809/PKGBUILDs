{
	"name": "ini",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"contributes": {
		"languages": [{
			"id": "ini",
			"extensions": [ ".ini", ".properties", ".gitconfig", ".desktop", ".directory", ".thumbnailer"],
